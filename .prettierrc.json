{
    "printWidth": 80,
    "singleQuote": true,
    "semi": false,
    "tabWidth": 2,
    "bracketSpacing": true,
    "trailingComma": "es5",
    "bracketSameLine": false,
    "useTabs": false,
    "endOfLine": "lf",
    "overrides": [
      {
        "files": "*.md",
        "options": {
          "singleQuote": false,
          "quoteProps": "preserve"
        }
      }
    ]
  }